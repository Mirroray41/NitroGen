{
  "schemaVersion": 1,
  "id": "nitrogen-core",
  "version": "${version}",

  "name": "NitroGen Core",
  "description": "This is a utility mod that allows you to choose what biomes you want in your world.",
  "authors": [
    "ZAppFireCZ"
  ],
  "contact": {
    "sources": "https://github.com/ZAppFireCZ/NitroGen"
  },

  "license": "CC0-1.0",
  "icon": "assets/nitrogen-core/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.zappfire.nitrogencore.NitroGenCore"
    ]
  },
  "mixins": [
    "nitrogen-core.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": "~1.19",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  },
  "accessWidener" : "nitrogen-core.accesswidener"
}
